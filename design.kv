<Content>:
    orientation: "vertical"
    spacing: "12dp"
    size_hint_y: None
    height: "120dp"
    id: content_dialog_change_pass
    MDTextField:
        id: old_pass
        hint_text: "Palavra-passe antiga"
        mode: "rectangle"
        required: False
        color: 1, 0, 1, 1
        line_color_focus: 0.5, 0, 1, 1
        icon_right: "shield-key"
        on_text: app.pass_old_change = self.text
        size_hint: .9, None
        password: True

    GridLayout:
        cols:2
        MDCheckbox:
            on_press: root.show_passold(self)
            size_hint: None, None
            height: 5
            width: 20

        MDLabel:
            text: "Mostrar Palavra-passe"
            font_size: "13px"
            color: 0,0,0,.4

    MDTextField:
        id: new_pass
        hint_text: "Palavra-passe nova"
        mode: "rectangle"
        required: False
        color: 1, 0, 1, 1
        line_color_focus: 0.5, 0, 1, 1
        on_text: app.pass_new_change = self.text
        icon_right: "shield-key"
        size_hint: .9, None
        password: True

    GridLayout:
        cols:2
        MDCheckbox:
            on_press: root.show_passnew(self)
            size_hint: None, None
            height: 10
            width: 20

        MDLabel:
            text: "Mostrar Palavra-passe"
            font_size: "13px"
            color: 0,0,0,.4

    GridLayout:
        cols: 2
        padding: 350, 8,0,0
        MDRaisedButton:
            text: "Cancelar"
            on_press: root.limpar_passes()
            md_bg_color: get_color_from_hex("#673AB7")
        MDRaisedButton:
            text: "Alterar"
            padding: 10, 10
            on_press: root.alterar_pass()
            md_bg_color: get_color_from_hex("#673AB7")

<ServersDown@BoxLayout>:
    name: "ServersDown"
    orientation: "horizontal"
    Image:
        source: "serversdown.png"
        size_hint: .3, .3
        pos_hint: {"center_x": 0.5, "center_y": 0.7}
    MDLabel:
        font_size: "15px"
        halign: "center"
        text: "Parece que algo nao correu como esperado...\nTente novamente mais tarde\nSe o problema persistir contacte o suporte técnico através de:\n18961@alunos.aescas.net"
    MDRaisedButton:
        text: "Tentar Novamente"
        on_press: root.tryagain()
        md_bg_color: 0.5, 0, 1, 1
        pos_hint: {"center_x": 0.5, "center_y": 0.4}
    MDSpinner:
        size_hint: None, None
        size: dp(46), dp(46)
        active: root.spin
        pos_hint: {"center_x": 0.5, "center_y": 0.3}
































<DashboardGerente@FloatLayout>:
    name: "DashboardGerente"
    on_pre_enter: root.close_menus(), root.profile_maker(), root.make_providers_list(), root.make_seccao_list(), root.make_empregados_list(),root.make_produtos_list(),root.make_encomendas_list()
    on_leave: root.close_menus(), root.remove_encomendas_list(), root.remove_providers_list(), root.remove_seccao_list(), root.remove_empregados_list(), root.remove_produtos_list()

    MDBottomNavigation:

        MDBottomNavigationItem: ## Perfil do utilizador
            name: 'Profile'
            text: 'Perfil'
            icon: 'account'
            badge_icon: "numeric-10"
            MDCard:
                border_radius: 20
                radius: [15]
                size_hint:None, None
                size: 300, 400
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                elevation: 10
                orientation: "vertical"
                AnchorLayout:
                    anchor_x: "left"
                    anchor_y: "top"
                    MDIconButton:
                        id:logout
                        text_color: 0,0,0,1
                        on_release: root.Logout()
                        icon: "account-arrow-left"

                GridLayout:
                    cols: 1
                    pos_hint: {"center_x": 0.5, "center_y": 0.5}
                    padding: 40,0,0,0
                    MDLabel:
                        text: "ID: " + root.id_utilizador
                    MDLabel:
                        text: "Nome: " + root.nome_utilizador
                    MDLabel:
                        text: "Cargo: " + root.cargo_utilizador
                    MDLabel:
                        text: "Salário: " + root.salario_utilizador

                MDRaisedButton:
                    opacity: 0
                MDRectangleFlatIconButton:
                    id: theme_changer
                    icon: "palette-outline"
                    text: "Alterar tema"
                    pos_hint: {"center_x": 0.5, "center_y": 0.5}
                    line_color: 0, 0, 0, 0
                    on_press: app.change_theme()
                MDRaisedButton:
                    opacity:0
                
                MDRaisedButton:
                    text: "Mudar palavra-passe"
                    md_bg_color: 0.5, 0, 1, 1
                    pos_hint: {"center_x": 0.5, "center_y": 0.5}
                    size_hint: .6, .4
                    on_release: root.show_alert_dialog(self)

                MDRaisedButton:
                    opacity: 0
                    text: "nadad"
                    size_hint: None, .1

                MDRaisedButton:
                    opacity: 0

        #Fim do perfil

        MDBottomNavigationItem:
            name: 'Providers'
            text: 'Fornecedores'
            icon: 'truck'
            MDLabel:
                text: "Não tem acesso a esta informação!"
                halign: "center"
            MDCard:
                disabled: app.provider_disable_box
                opacity: app.provider_opacity
                size_hint:None, None
                size: 1100, 400
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                elevation: 15
                padding: 10
                orientation: "vertical"
                
                BoxLayout:
                    cols: 6
                    size_hint_y: None
                    size_hint_x: 1
                    padding: 10, 10
                    size_hint_x:None 
                    width:1100
