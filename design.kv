            source: "logo_xenox.png"
            size_hint: None, None
            size: 75, 75
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
        MDLabel:
            text:"Software de Gestão de Stocks"
            font_size: 10
            halign: "center"
            height: 0
        MDTextField:
            id: text_field_user
            mode: "rectangle"
            required: False
            color_mode: 'custom'
            color: 1, 0, 1, 1
            hint_text: "Nome de Utilizador"
            line_color_focus: 0.5, 0, 1, 1
            size_hint: .9, None
            pos_hint: {"center_x": 0.5}
            icon_right: "account"

        MDTextField:
            id: text_field_pass
            required: False
            mode: "rectangle"
            color_mode: 'custom'
            color: 0.5, 0, 1, 1
            hint_text: "Palavra-passe" 
            line_color_focus: 0.5, 0, 1, 1
            size_hint: .9, None
            pos_hint: {"center_x": 0.5}
            icon_right: 'key-variant'
            password: True
            
        GridLayout:
            cols:2
            MDCheckbox:
                on_press: root.show_pass(self)
                size_hint: .1, .1
                height: 20

            MDLabel:
                text: "Mostrar Palavra-passe"
                font_size: "13px"
                color: 0,0,0,.4

        MDRaisedButton:
            text: "Login"
            md_bg_color: 0.5, 0, 1, 1
            on_press: root.validate_login()
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            
        MDRaisedButton:
            opacity: 0
        MDRaisedButton:
            opacity: 0
        MDRaisedButton:
            opacity: 0

        
            
